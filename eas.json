{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development",
        "API_URL": "https://chatflow.pabbly.com/api",
        "SOCKET_URL": "https://chatflow.pabbly.com/",
        "PABBLY_ACCOUNTS_URL": "https://accounts.pabbly.com",
        "PABBLY_ACCOUNTS_BACKEND_URL": "https://accounts.pabbly.com/backend",
        "PABBLY_PROJECT": "pcf",
        "DEBUG_MODE": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "preview",
        "API_URL": "https://chatflow.pabbly.com/api",
        "SOCKET_URL": "https://chatflow.pabbly.com/",
        "PABBLY_ACCOUNTS_URL": "https://accounts.pabbly.com",
        "PABBLY_ACCOUNTS_BACKEND_URL": "https://accounts.pabbly.com/backend",
        "PABBLY_PROJECT": "pcf",
        "DEBUG_MODE": "false"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "autoIncrement": true
      },
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production",
        "API_URL": "https://chatflow.pabbly.com/api",
        "SOCKET_URL": "https://chatflow.pabbly.com/",
        "PABBLY_ACCOUNTS_URL": "https://accounts.pabbly.com",
        "PABBLY_ACCOUNTS_BACKEND_URL": "https://accounts.pabbly.com/backend",
        "PABBLY_PROJECT": "pcf",
        "DEBUG_MODE": "false"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "your-apple-id@email.com",
        "ascAppId": "your-app-store-connect-app-id"
      }
    }
  }
}
